{
  "name": "@stinkstudios/boilerplate-react-components",
  "version": "0.0.1",
  "description": "A collection of React components to be used on Stink Studios' React Boilerplate",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint",
    "lint:css": "stylelint ./components/**/*.css",
    "lint:eslint": "eslint --ext .js ./components --cache",
    "lint-staged": "echo 'Running lint-staged for staged files...' && lint-staged",
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server --hot --inline --color --content-base ./ ",
    "build:webpack": "NODE_ENV=production webpack --progress --color -p "
  },
  "lint-staged": {
    "*(*.js|*.jsx)": "eslint",
    "*(*.css|*.scss)": "stylelint"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stinkstudios/boilerplate-react-components.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "components",
    "stink",
    "studios"
  ],
  "author": "Matteo Belfiore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stinkstudios/boilerplate-react-components/issues"
  },
  "homepage": "https://github.com/Stinkstudios/boilerplate-react-components#readme",
  "dependencies": {
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2",
    "svg-inline-react": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx": "6.23.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-regenerator": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.2.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "eslint": "3.17.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-compat": "1.0.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "html-webpack-plugin": "^2.22.0",
    "lint-staged": "3.3.1",
    "postcss-loader": "1.3.3",
    "postcss-scss": "0.4.1",
    "pre-commit": "1.2.2",
    "stylelint": "7.9.0",
    "stylelint-order": "0.3.0",
    "stylelint-scss": "1.4.3",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "peerDependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2"
  }
}
