{
  "baseBranch": ["master"],
  "commitMessagePrefix": "feat(deps):",
  "extends": ["config:base"],
  "labels": ["feat(deps): dependency update"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "rangeStrategy": "bump",
  "schedule": ["after 10pm and before 5am on every weekday", "every weekend"],
  "timezone": "Europe/London",
  "ignorePaths": ["depreciated/*"],
  "automerge": true,
  "major": {
    "automerge": false
  }
}
