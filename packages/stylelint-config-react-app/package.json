{
  "name": "@stinkstudios/stylelint-config-react-app",
  "version": "1.0.1",
  "description": "@stinkstudios Create React App shareable config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "author": "Andy Kenward <hello@andykenward.com>",
  "homepage": "https://github.com/Stinkstudios/npm-packages/tree/master/packages/stylelint-config-react-app#readme",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "index.js",
    "no-unsupported-browser-features.js",
    "order.js",
    "properties-order.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stinkstudios/npm-packages.git"
  },
  "scripts": {
    "lint:js": "eslint '**/*.js'",
    "lint": "yarn lint:js && yarn stylelint-check",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "jest --env=node",
    "test:watch": "jest --env=node --watch"
  },
  "bugs": {
    "url": "https://github.com/Stinkstudios/npm-packages/issues"
  },
  "devDependencies": {
    "eslint": "5.7.0",
    "eslint-config-stylelint": "9.0.0",
    "jest": "23.6.0",
    "stylelint": "^9.6.0"
  },
  "peerDependencies": {
    "stylelint": "^9.6.0"
  },
  "dependencies": {
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-order": "^1.0.0"
  }
}
