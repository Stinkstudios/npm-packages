{
  "name": "@stinkstudios/cf-auth",
  "version": "1.0.0",
  "description": "Module to password protect CloudFront distributions",
  "homepage": "https://stinkstudios.com",
  "author": {
    "name": "Patrick Arminio",
    "email": "patrick.arminio@gmail.com",
    "url": "https://patrick.wtf"
  },
  "license": "MIT",
  "repository": "Stinkstudios/npm-packages",
  "bugs": {
    "url": "https://github.com/Stinkstudios/npm-packages/issues"
  },
  "keywords": [
    "cloudfront",
    "authentication",
    "aws",
    "lambda"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.5",
    "@types/node": "10.12.0",
    "coveralls": "3.0.2",
    "jest": "23.6.0",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "__tests__/.*(test|spec)\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
