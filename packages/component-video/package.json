{
  "name": "@stinkdigital/component-video",
  "version": "0.1.3",
  "description": "A simple video component for videos and youtube videos.",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib/ dist/ es/",
    "check:src": "npm run lint",
    "lint:fix": "eslint -c .eslintrc --fix src/**",
    "lint": "eslint -c .eslintrc src/**",
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server --hot --inline --colors --error-details  --content-base example/ ",
    "host": "webpack-dev-server --hot --inline --colors --error-details  --content-base example/  --host '192.168.10.4'",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:webpack": "NODE_ENV=production webpack --progress --colors  --error-details",
    "build": "npm run build:es && npm run build:commonjs && npm run build:webpack",
    "prepublish": "npm run clean && npm run check:src && npm run build:es && npm run build:commonjs"
  },
  "engines": {
    "node": "5.7.1",
    "npm": "3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stinkdigital/component-video.git"
  },
  "author": "Stinkdigital",
  "contributors": [
    {
      "name": "Andy Kenward",
      "email": "andykenward@gmail.com",
      "url": "https://github.com/andykenward"
    },
    {
      "name": "Yi-wen LIN",
      "url": "https://github.com/yiwenl"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stinkdigital/component-video/issues"
  },
  "homepage": "https://github.com/Stinkdigital/component-video#readme",
  "dependencies": {
    "@stinkdigital/detector": "0.0.4",
    "load-script": "^1.0.0"
  },
  "peerDependencies": {
    "@stinkdigital/detector": "git+ssh://git@github.com/stinkdigital/detector.git#0.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.5",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-check-es2015-constants": "6.7.2",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.5.2",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.6.5",
    "babel-plugin-transform-es2015-block-scoping": "6.7.1",
    "babel-plugin-transform-es2015-classes": "6.6.5",
    "babel-plugin-transform-es2015-computed-properties": "6.6.5",
    "babel-plugin-transform-es2015-destructuring": "6.6.5",
    "babel-plugin-transform-es2015-for-of": "6.6.0",
    "babel-plugin-transform-es2015-function-name": "6.5.0",
    "babel-plugin-transform-es2015-literals": "6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.7.4",
    "babel-plugin-transform-es2015-object-super": "6.6.5",
    "babel-plugin-transform-es2015-parameters": "6.7.0",
    "babel-plugin-transform-es2015-shorthand-properties": "6.5.0",
    "babel-plugin-transform-es2015-spread": "6.6.5",
    "babel-plugin-transform-es2015-sticky-regex": "6.5.0",
    "babel-plugin-transform-es2015-template-literals": "6.6.5",
    "babel-plugin-transform-es2015-unicode-regex": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "eslint-config-stinkdigital": "0.0.8",
